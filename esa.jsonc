{
  "version": 2,
  "staticAssets": {
    "assets": [
      {
        "src": "/*",
        "dest": "/"
      },
      {
        "src": "/css/*",
        "dest": "/css/"
      },
      {
        "src": "/js/*",
        "dest": "/js/"
      },
      {
        "src": "/images/*",
        "dest": "/images/"
      }
    ]
  }
}